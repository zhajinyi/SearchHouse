<result name="list3">/page/html/AllHouse.jsp</result></action>  
        
<action name="getHouseById" class="com.genminno.action.HouseAction" method="getHouseById">
</action>  
      
<action name="updateHouse" class="com.genminno.action.HouseAction" method="updateHouse">
<result name="update" type="chain">queryAllhouses</result></action>

